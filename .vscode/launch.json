{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Main Process",
            "type": "node",
            "request": "launch",
            "cwd": "${workspaceRoot}",
            "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron",
            "program": "${workspaceRoot}/app/main.dev.js",
            "runtimeArgs": ["-r", "babel-register"],
            "env": {
                "HOT": "1",
                "NODE_ENV": "development"
            }
        }
    ]
}
